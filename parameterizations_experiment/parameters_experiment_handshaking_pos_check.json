{
	"situation_definition_fname" : "situation_definitions/handshaking.json",
			
	"situate_parameterizations_fnames" : [
				"parameterizations_run/parameters_situate_homebrew_rcnn_priming.json"
				//"parameterizations_run/parameters_situate_faster_rcnn_priming.json"
				//"parameterizations_run/parameters_situate_uniform.json"
			],	

	"experiment_settings" : {

			"description" : "handshaking_positives_check",

			// full path, or relative to base_image_directories
			"directory_train" : "Handshaking_train/",
			"directory_test"  : "Handshaking_train/",

			"use_parallel"					: 0, 
			
			"run_analysis_after_completion"	: 0,

			"use_visualizer" : 0,

			// additional visualization options
		 	"viz_options" : {
		 		"on_iteration_mod"    : 1,
		 		"on_workspace_change" : 0,
		 		"on_end"              : 1,
		 		"start_paused"        : 1
	 		},

			// training / testing image lists
			// 	if directory_train and directory_test are the same, 
			// 	then the division of images specified in this directory will be used
			"training_testing_split_directory" : "handshaking_validation",

			// number of folds
			// 	if image lists were provided
			// 		if this is empty, then all of them will be used.
			//  	if this is not empty, it will be a limit on how many to use.
			//	if image list is not provided, this will be used when generating 
			//		training/testing image lists
			"num_folds" : 1,

			"specific_folds" : [1],

			"max_testing_images" : 10,
			
			// if specified, this will be used to seed the RNG during testing.
			// otherwise, each parameterization will get a different time-based seed
			"testing_seed"  : ""

	}

}